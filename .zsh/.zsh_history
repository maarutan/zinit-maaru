echo "" > braces.zsh
echo "# Function to insert pairs of characters\
insert-pairs() {\
    # Get the character being typed, defaulting to the last character in the LBUFFER\
    local char="${KEYS:-$LBUFFER[-1]}"\
    \
    # Define pairs of characters for insertion\
    local pairs=(\
        "{ }" "[ ]" "( )" "< >" "' '" '\" \"' "` `"\
    )\
    \
    # Iterate through each pair to check if the current character is an opening character\
    for pair in "${pairs[@]}"; do\
        local left="${pair%% *}"  # Extract the opening character\
        local right="${pair##* }" # Extract the closing character\
        if [[ "$char" == "$left" ]]; then\
            # If it's an opening character, insert both the opening and closing characters\
            LBUFFER+="$left$right"\
            CURSOR=$((CURSOR - 1)) # Move the cursor back between the characters\
            return\
        fi\
    done\
    \
    # If not a pair, just append the character to the buffer\
    LBUFFER+="$char"\
}\
\
# Function to delete pairs of characters\
delete-pairs() {\
    # Define pairs of characters for deletion\
    local pairs=(\
        "{ }" "[ ]" "( )" "< >" "' '" '\" \"' "` `"\
    )\
    \
    # Get the character to the left and right of the cursor\
    local left="${LBUFFER[-1]}"\
    local right="${RBUFFER[1]}"\
    \
    # Combine the left and right characters\
    local surrounding="${LBUFFER[-1]}${RBUFFER[1]}"\
\
    # Check if the surrounding characters form a valid pair\
    for pair in "${pairs[@]}"; do\
        local open="${pair%% *}"  # Extract the opening character\
        local close="${pair##* }" # Extract the closing character\
        if [[ "$surrounding" == "$open$close" ]]; then\
            # If a valid pair is found, remove only the left character\
            LBUFFER="${LBUFFER%?}"\
            RBUFFER="${RBUFFER#?}"\
            return\
        fi\
    done\
\
    # If not a valid pair, check for an unbalanced situation\
    if [[ -n "$left" && -n "$right" ]]; then\
        for pair in "${pairs[@]}"; do\
            local open="${pair%% *}"  # Extract the opening character\
            local close="${pair##* }" # Extract the closing character\
            if [[ "$left" == "$open" || "$right" == "$close" ]]; then\
                # If unbalanced, delete both left and right characters\
                LBUFFER="${LBUFFER%?}"\
                RBUFFER="${RBUFFER#?}"\
                return\
            fi\
        done\
    fi\
\
    # If no pair is found, perform a standard backspace operation\
    zle backward-delete-char\
}\
\
# Register the functions as Zle widgets\
zle -N insert-pairs\
zle -N delete-pairs\
\
# Bind keys to insert-pairs for specific opening characters\
bindkey '{' insert-pairs\
bindkey '[' insert-pairs\
bindkey '(' insert-pairs\
bindkey '<' insert-pairs\
bindkey "'" insert-pairs\
bindkey '"' insert-pairs\
bindkey '`' insert-pairs\
\
# Bind Backspace to delete-pairs\
bindkey '^?' delete-pairs\
" > braces.zsh
